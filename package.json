{
  "name": "mqdockerup",
  "version": "1.0.3",
  "description": "A Node.js application that pushes information about running Docker containers to an MQTT server.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "watch": "tsc -w",
    "build": "npx tsc",
    "start": "node dist/src/index.js",
    "start:build": "npx tsc && node dist/src/index.js",
    "start:dev": "nodemon --watch 'src/*' -e ts --exec 'npx tsc && node dist/src/index.js'"
  },
  "author": "Michel Ferreira Ribeiro",
  "license": "MIT",
  "keywords": [
    "docker",
    "nodejs",
    "mqtt",
    "websockets",
    "images",
    "version",
    "update"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "dockerode": "^3.3.5",
    "figlet": "^1.6.0",
    "fs": "^0.0.1-security",
    "mqtt": "^4.3.7",
    "winston": "^3.10.0",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.19",
    "@types/figlet": "^1.5.6",
    "@types/jest": "^29.5.2",
    "install": "^0.13.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "npm": "^9.8.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
