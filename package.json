{
  "name": "mqdockerup",
  "version": "0.1.2",
  "description": "A Node.js application that pushes information about running Docker containers to an MQTT server.",
  "main": "index.js",
  "scripts": {
    "test": "echo 'No tests defined yet.",
    "watch": "tsc -w",
    "build": "npx tsc",
    "start": "node dist/src/index.js",
    "start:build": "npx tsc && node dist/src/index.js",
    "start:dev": "nodemon --watch 'src/*' -e ts --exec 'npx tsc && node dist/src/index.js'"
  },
  "author": "Michel Ferreira Ribeiro",
  "license": "MIT",
  "keywords": [
    "docker",
    "nodejs",
    "mqtt",
    "websockets",
    "images",
    "version",
    "update"
  ],
  "dependencies": {
    "axios": "^1.3.2",
    "dockerode": "^3.3.4",
    "fs": "^0.0.1-security",
    "mqtt": "^4.3.7",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "@types/dockerode": "^3.3.14",
    "nodemon": "^2.0.20"
  }
}
