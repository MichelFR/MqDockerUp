{
  "name": "mqdockerup",
  "version": "0.16.4",
  "description": "A Node.js application that pushes information about running Docker containers to an MQTT server.",
  "main": "index.js",
  "scripts": {
    "test": "echo 'No tests defined yet.'",
    "watch": "tsc -w",
    "build": "npx tsc",
    "start": "node dist/src/index.js",
    "start:build": "npx tsc && node dist/src/index.js",
    "start:dev": "nodemon --watch 'src/*' -e ts --exec 'npx tsc && node dist/src/index.js'"
  },
  "author": "Michel Ferreira Ribeiro",
  "license": "MIT",
  "keywords": [
    "docker",
    "nodejs",
    "mqtt",
    "websockets",
    "images",
    "version",
    "update"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "dockerode": "^3.3.5",
    "figlet": "^1.6.0",
    "fs": "^0.0.1-security",
    "mqtt": "^4.3.7",
    "winston": "^3.9.0",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.18",
    "@types/figlet": "^1.5.6",
    "install": "^0.13.0",
    "nodemon": "^2.0.22",
    "npm": "^9.7.1",
    "source-map-support": "^0.5.21",
    "typescript": "^5.1.3"
  }
}
